#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/tailieu/SEP490/New folder/SEP490_FARMITY/server/node_modules/.pnpm/mkcert@3.2.0/node_modules/mkcert/dist/node_modules:/mnt/d/tailieu/SEP490/New folder/SEP490_FARMITY/server/node_modules/.pnpm/mkcert@3.2.0/node_modules/mkcert/node_modules:/mnt/d/tailieu/SEP490/New folder/SEP490_FARMITY/server/node_modules/.pnpm/mkcert@3.2.0/node_modules:/mnt/d/tailieu/SEP490/New folder/SEP490_FARMITY/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/tailieu/SEP490/New folder/SEP490_FARMITY/server/node_modules/.pnpm/mkcert@3.2.0/node_modules/mkcert/dist/node_modules:/mnt/d/tailieu/SEP490/New folder/SEP490_FARMITY/server/node_modules/.pnpm/mkcert@3.2.0/node_modules/mkcert/node_modules:/mnt/d/tailieu/SEP490/New folder/SEP490_FARMITY/server/node_modules/.pnpm/mkcert@3.2.0/node_modules:/mnt/d/tailieu/SEP490/New folder/SEP490_FARMITY/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkcert/dist/cli.js" "$@"
else
  exec node  "$basedir/../mkcert/dist/cli.js" "$@"
fi
